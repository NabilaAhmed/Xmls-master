<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <CheckBox
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:text="NotifyMe"

        android:textSize="20dp"
        android:id="@+id/checkBox2"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="220dp"
        android:layout_height="220dp"
        android:text="Report_view"
        android:textSize="20dp"
        android:id="@+id/textView21"
        android:layout_below="@+id/checkBox2"
        android:layout_alignRight="@+id/checkBox2"
        android:layout_alignEnd="@+id/checkBox2" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView2"
        android:layout_alignTop="@+id/textView21"
        android:layout_toStartOf="@+id/textView21"
        android:layout_toLeftOf="@+id/textView21" />

    <EditText
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:text="My chlid coursers"
        android:textSize="20dp"
        android:id="@+id/editText10"
        android:layout_above="@+id/listView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
